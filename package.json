{
  "name": "100menvsgorilla",
  "version": "1.0.0",
  "description": "A 3D game where 100 men battle a gorilla",
  "scripts": {
    "install:all": "npm install && npm --prefix client install && npm --prefix server install",
    "client:dev": "npm --prefix client run dev",
    "client:build": "npm --prefix client run build",
    "server:dev": "npm --prefix server run dev",
    "server:start": "npm --prefix server run start",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build",
    "start": "npm run server:start"
  },
  "keywords": [
    "game",
    "3d",
    "threejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "howler": "^2.2.3",
    "ngrok": "4.3.3",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "three": "^0.151.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
